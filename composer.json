{
    "name": "rcconsulting/myfmapilibrary-for-php",
    "type": "library",
    "description": "FileMaker 17/18 API PHP wrapper",
    "Keywords": ["FileMaker", "API", "REST", "Database", "Wrapper"],
    "license": "gpl-3.0",
    "version": "1.1.5",
    "minimum-stability": "stable",
    "authors": [
      {
        "name": "Lesterius",
        "email": "infoFR@lesterius.com",
        "homepage": "https://www.lesterius.com"
      },
       {
           "name": "Jacob Taylor",
           "email": "jacob.taylor@rcconsulting.com",
           "homepage": "https://rcconsulting.com"
       },
       {
           "name": "Steve Allen",
           "email": "steve.allen@rcconsulting.com",            
           "homepage": "https://rcconsulting.com"
        }
    ],
    "autoload": {
        "psr-4": {
          "Lesterius\\FileMakerApi\\": "src",
          "Tests\\": "tests/"
        }
    },
    "require": {
        "php" : ">=5.5.0",
        "ext-curl": "*",
        "ext-mbstring": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.6"
    },
    "config": {
      "sort-packages": true,
      "optimize-autoloader": true
    }
}
